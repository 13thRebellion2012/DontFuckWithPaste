{
  "name": "Don't fuck with paste",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Prevents the blocking of pasting into input fields",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": [ "*://*.google.com/*" ],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}
